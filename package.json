{
  "name": "jarvis-memory",
  "version": "1.0.0",
  "description": "OpenClaw plugin for long-term memory with pgvector and local embeddings",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src/",
    "clean": "rm -rf dist/"
  },
  "keywords": [
    "openclaw",
    "plugin",
    "memory",
    "pgvector",
    "embeddings"
  ],
  "author": "Danny",
  "license": "MIT",
  "openclaw": {
    "type": "plugin",
    "tools": [
      "memory_search",
      "memory_store",
      "memory_forget"
    ]
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.0",
    "postgres": "^3.4.4",
    "pgvector": "^0.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
